{
  "version": 1,
  "canisters": {
    "auth_canister": {
      "type": "rust",
      "package": "auth_canister",
      "candid": "backend/auth_canister/auth_canister.did"
    },
    "asset_canister": {
      "type": "rust",
      "package": "land_registry_canister",
      "candid": "backend/asset_canister/land_registry.did"
    },
    "marketplace_canister": {
      "type": "rust",
      "package": "marketplace_canister",
      "candid": "backend/marketplace_canister/marketplace_canister.did"
    },
    "frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "build": "cd frontend && npm run build"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "frontend": {}
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  }
}
